{
  "apiVersion": "nais.io/v1alpha1",
  "kind": "Application",
  "metadata": {
    "name": "finn-kandidat-api",
    "namespace": "default",
    "labels": {
      "team": "teamtag"
    }
  },
  "spec": {
    "image": "navikt/finn-kandidat-api:latest",
    "team": "teamtag",
    "port": 8080,
    "vault": { "enabled": true },
    "liveness": {
      "path": "/finn-kandidat-api/internal/actuator/info",
      "initialDelay": 20
    },
    "readiness": {
      "path": "/finn-kandidat-api/internal/actuator/info",
      "initialDelay": 20
    },
    "ingresses": [
      "https://arbeidsgiver.nais.adeo.no/finn-kandidat-api/"
    ],
    "env": [
      {
        "name": "SPRING_PROFILES_ACTIVE",
        "value": "prod"
      }
    ],
    "webproxy": true,
    "prometheus": {
      "enabled": true,
      "path": "/finn-kandidat-api/internal/actuator/prometheus"
    }
  }
}
